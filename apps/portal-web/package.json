{
  "name": "@scow/portal-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_USE_MOCK=1 next dev",
    "devapi": "cross-env NEXT_PUBLIC_USE_MOCK=0 next dev",
    "serve": "next start",
    "build": "pnpm prepareDev && npm run build:nogen",
    "build:nogen": "cross-env BUILDING=1 next build",
    "prepareDev": "pnpm protos && ntar schema && pnpm cloneNoVnc",
    "protos": "tsgrpc-cli protos",
    "cloneNoVnc": "git -C ./public/vnc pull || git clone -c advice.detachedHead=false --depth=1 --branch v1.3.0 https://github.com/novnc/noVNC ./public/vnc"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@ddadaal/next-typed-api-routes-runtime": "0.3.2",
    "@ddadaal/tsgrpc-utils": "0.14.0",
    "@scow/lib-decimal": "workspace:*",
    "@scow/config": "workspace:*",
    "@grpc/grpc-js": "1.6.7",
    "@newhighsco/next-plugin-svgr": "3.0.50",
    "@sinclair/typebox": "0.23.5",
    "antd": "4.20.7",
    "google-protobuf": "3.20.1",
    "http-proxy": "1.18.1",
    "less": "4.1.2",
    "next": "12.1.6",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "9.0.0",
    "nookies": "2.5.2",
    "nprogress": "0.2.0",
    "react": "18.1.0",
    "react-async": "10.0.1",
    "react-dom": "18.1.0",
    "simstate": "3.0.1",
    "styled-components": "5.3.5",
    "tslib": "2.4.0",
    "typescript": "4.7.2",
    "xterm": "4.18.0",
    "xterm-addon-fit": "0.5.0",
    "socket.io-client": "4.5.1",
    "mime-types": "2.1.35",
    "react-is": "18.1.0",
    "moment": "2.29.3",
    "@codemirror/state": "0.20.0",
    "@codemirror/view": "0.20.7",
    "@codemirror/basic-setup": "0.20.0",
    "@codemirror/legacy-modes": "0.20.0",
    "@codemirror/theme-one-dark": "0.20.0",
    "@codemirror/language": "0.20.2",
    "@codemirror/commands": "0.20.0",
    "@codemirror/autocomplete": "0.20.3",
    "socket.io": "4.5.1",
    "node-pty": "0.11.0-beta9",
    "random-words": "1.1.2"
  },
  "devDependencies": {
    "@ddadaal/next-typed-api-routes-cli": "0.4.0",
    "@ddadaal/tsgrpc-cli": "0.13.0",
    "@next/bundle-analyzer": "12.1.6",
    "@types/google-protobuf": "3.15.6",
    "@types/node": "17.0.35",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.10",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.25",
    "@types/url-join": "4.0.1",
    "@types/random-words": "1.1.2",
    "postcss": "8.4.14",
    "webpack": "5.72.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}

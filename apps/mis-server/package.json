  "main": "build/index.js",
  "scripts": {
    "dev": "dotenv -e env/.env.dev -- node -r ts-node/register -r tsconfig-paths/register --watch src/index.ts",
    "build": "rimraf build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "serve": "node build/index.js",

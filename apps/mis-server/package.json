    "@scow/lib-scheduler-adapter": "workspace:*",
    "@scow/utils": "workspace:*",
    "@sinclair/typebox": "0.28.9",
    "dotenv": "16.0.3",
    "node-cron": "3.0.2",
    "pino": "8.14.1",
    "pino-pretty": "10.0.0",
    "uuid": "9.0.0",
    "wait-on": "7.0.1"

{
  "name": "@scow/ai",
  "version": "0.1.0",
  "private": true,
  "files": [
    ".next",
    "public",
    "next.config.mjs",
    "build"
  ],
  "scripts": {
    "dev": "dotenv -e env/.env.dev -- node --watch --loader ts-paths-esm-loader server/index.ts",
    "dev:server": "cross-env NEXT_PUBLIC_USE_MOCK=0 next dev",
    "build": "npm run build:next",
    "build:next": "cross-env BUILDING=1 next build",
    "build:ts": "rimraf build && tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "serve": "node build/server/index.js",
    "orm": "dotenv -e env/.env.dev -- npx mikro-orm",
    "ormCreate": "dotenv -e env/.env.dev -- npx mikro-orm migration:create"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/server/mikro-orm.config.ts",
      "./src/server/mikro-orm.config.js"
    ]
  },
  "dependencies": {

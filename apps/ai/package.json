{
  "name": "@scow/ai",
  "version": "0.3.0",
  "private": true,
  "files": [
    ".next",
    "public",
    "next.config.mjs",
    "build",
    "assets",
    "tsconfig.json",
    "tsconfig.server.json",
    "start.mjs"
  ],
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_USE_MOCK=1 next dev",
    "dev:server": "cross-env NEXT_PUBLIC_USE_MOCK=0 next dev",
    "serve": "node start.mjs",
    "serve:next": "next start",
    "build": "node scripts/copyMonacoToPublic.js && npm run build:next",
    "build:next": "rimraf .next.backup && cross-env BUILDING=1 NEXT_PUBLIC_BASE_PATH=/@BASE_PATH@ next build --no-mangling",
    "build:ts": "rimraf build && tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "orm": "dotenv -e env/.env.dev -- npx mikro-orm",
    "ormCreate": "dotenv -e env/.env.dev -- npx mikro-orm migration:create",
    "ormUp": "dotenv -e env/.env.dev -- npx mikro-orm migration:up",
    "prepareDev": "node scripts/copyMonacoToPublic.js",
    "lint": "eslint ."
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/server/mikro-orm.config.ts",
      "./src/server/mikro-orm.config.js"
    ]
  },

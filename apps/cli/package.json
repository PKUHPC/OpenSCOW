{
  "name": "@scow/cli",
  "version": "1.8.1",
  "description": "CLI to manage SCOW",
  "main": "build/index.js",
  "bin": "build/index.js",
  "private": true,
  "scripts": {
    "dev": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "build": "rimraf build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && pkg --compress GZip --no-bytecode --public-packages \"*\" --public .",
    "test": "jest",
    "lint": "eslint -c ../../eslint.config.js ."
  },
  "dependencies": {
    "@octokit/rest": "20.1.1",
    "@scow/config": "workspace:*",
    "@scow/lib-config": "workspace:*",
    "@sinclair/typebox": "0.34.13",
    "death": "1.1.0",
    "dotenv": "16.4.7",
    "https-proxy-agent": "7.0.6",
    "js-yaml": "4.1.0",
    "jszip": "3.10.1",
    "node-fetch-commonjs": "3.3.2",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "pkg": "5.8.1",
    "prompts": "2.4.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/death": "1.1.5",
    "@types/js-yaml": "4.0.9",
    "@types/prompts": "2.4.9",
    "@types/yargs": "17.0.33"
  },
  "author": "PKUHPC (https://github.com/PKUHPC)",
  "license": "Mulan PSL v2",
  "repository": "https://github.com/PKUHPC/SCOW",
  "keywords": [],
  "volta": {
    "extends": "../../package.json"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "assets/**/*",
      "package.json",
      "node_modules/@sinclair/typebox/**/*"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-linux-arm64"
    ],
    "outputPath": "exe"
  }
}

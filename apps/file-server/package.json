{
  "name": "@scow/file-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "build/index.js",
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json && pkg .",
    "dev": "nodemon --watch src -e ts --exec \"npx dotenv -e env/.env.dev -- npx ts-node -r tsconfig-paths/register src/index.ts\"",
    "test": "jest",
    "genApi": "dotenv -e env/.env.dev -- ts-node -r tsconfig-paths/register src/genSchema.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@scow/config": "workspace:*",
    "@scow/lib-auth": "workspace:*",
    "@grpc/grpc-js": "1.6.7",
    "@sinclair/typebox": "0.23.5",
    "fast-json-stringify": "4.2.0",
    "fastify": "4.1.0",
    "@fastify/error": "3.0.0",
    "fastify-graceful-shutdown": "3.1.0",
    "fastify-plugin": "3.0.1",
    "nanoid": "4.0.0",
    "@fastify/formbody": "7.0.1",
    "@fastify/static": "6.4.0",
    "@fastify/cookie": "7.0.0",
    "@fastify/multipart": "7.1.0",
    "shell-quote": "1.7.3",
    "mime-types": "2.1.35"
  },
  "devDependencies": {
    "@ddadaal/tsgrpc-cli": "0.13.0",
    "form-data": "4.0.0",
    "@types/shell-quote": "1.7.1",
    "@types/mime-types": "2.1.1",
    "@fastify/swagger": "7.4.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node16-linux-x64"
    ],
    "outputPath": "build"
  }
}

{
  "name": "ai-web",
  "version": "0.1.0",
  "private": true,
  "files": [
    ".next",
    "public",
    "next.config.js",
    "build"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "demo": "./quick-demo.sh",
    "lint": "next lint",
    "lint:types": "tsc --noEmit",
    "test:compose-up": "docker compose -f docker-compose.test.yml up --wait --remove-orphans",
    "test:compose-down": "docker compose -f docker-compose.test.yml down",
    "test": "pnpm test:compose-up && (ava ; pnpm test:compose-down)",
    "coverage": "pnpm test:compose-up && (c8 ava ; pnpm test:compose-down)",
    "report:html": "pnpm test:compose-up && ((c8 ava && c8 report --reporter=html) ; pnpm test:compose-down)",
    "ci": "c8 ava && c8 report --reporter=json",
    "orm": "dotenv -e env/.env.dev -- npx mikro-orm",
    "ormCreate": "dotenv -e env/.env.dev -- npx mikro-orm migration:create"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/server/mikro-orm.config.ts",
      "./src/server/mikro-orm.config.js"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.17.0",
    "@ant-design/icons": "5.2.6",
    "@mikro-orm/cli": "5.9.4",
    "@mikro-orm/core": "5.9.4",
    "@mikro-orm/migrations": "5.9.4",
    "@mikro-orm/mysql": "5.9.4",
    "@scow/config": "workspace:*",
    "@scow/lib-config": "workspace:*",
    "@scow/lib-decimal": "workspace:*",
    "@scow/utils": "workspace:*",
    "@sinclair/typebox": "0.31.10",
    "@trpc/client": "10.44.1",
    "@trpc/next": "10.44.1",
    "@trpc/react-query": "10.44.1",
    "@trpc/server": "10.44.1",
    "antd": "5.8.6",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "http-proxy": "1.18.1",
    "lodash": "4.17.21",
    "long": "5.2.3",
    "mime-types": "2.1.35",
    "next": "14.0.3",
    "next-connect": "1.0.0",
    "nextjs-cors": "^2.1.2",
    "nookies": "2.5.2",
    "nprogress": "0.2.0",
    "protobufjs": "7.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "simstate": "3.0.1",
    "styled-components": "6.0.7",
    "superjson": "2.2.1",
    "swagger-ui-react": "5.10.3",
    "trpc-openapi": "1.2.0",
    "ws": "8.13.0",
    "xterm": "5.2.1",
    "xterm-addon-fit": "0.7.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/http-proxy": "1.17.11",
    "@types/mime-types": "2.1.1",
    "@types/node": "18.17.14",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "5.1.26",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/ws": "8.5.5",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}

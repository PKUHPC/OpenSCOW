{
  "typescript.tsdk": "node_modules/typescript/lib",
  "protoc": {
    "options": [
      "--proto_path=./protos"
    ],
  },
  "buf.binaryPath": "node_modules/@bufbuild/buf/bin/buf",
  "cSpell.words": [
    "trpc"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
}
